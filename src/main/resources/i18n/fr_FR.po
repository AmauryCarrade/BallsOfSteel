msgid "{aqua}------ Team commands ------"
msgstr "{aqua}------ Commandes liées aux équipes ------"

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:92
msgid "{cc}/bos team add <color> {ci}: adds a team with the provided color."
msgstr ""
"{cc}/bos team add <couleur> {ci}: ajoute une équipe de la couleur donnée."

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:93
msgid ""
"{cc}/bos team add <color> <name ...> {ci}: adds a named team with the "
"provided name and color."
msgstr ""
"{cc}/bos team add <couleur> <nom ...> {ci}: ajoute une équipe avec le nom et "
"la couleur donnés."

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:94
msgid "{cc}/bos team remove <name ...> {ci}: removes a team"
msgstr "{cc}/bos team remove <nom ...> {ci}: supprime une équipe."

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:95
msgid ""
"{cc}/bos team spawn [x,y,z | x,z] <name ...> {ci}: sets the spawn point of "
"the team (location of the sender or coordinates)."
msgstr ""
"{cc}/bos team spawn [x,y,z | x,z] <nom ...> {ci}: enregistre le point de "
"démarrage de l'équipe (localisation de l'envoyeur, ou coordonnées)."

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:96
msgid ""
"{cc}/bos team chest [x,y,z] <name ...> {ci}: sets the chest of this team "
"(where the diamonds will be stored), using the given coordinates or the "
"block the sender is looking at."
msgstr ""
"{cc}/bos team chest [x,y,z] <name ...> {ci}: enregistre le coffre de "
"l'équipe (où seront stockés les diamants), en utilisant les coordonnées "
"données ou le bloc regardé par l'envoyeur."

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:97
msgid ""
"{cc}/bos team join <player> <teamName ...>{ci}: adds a player inside the "
"given team. The name of the team is it color, or the explicit name given."
msgstr ""
"{cc}/bos team join <joueur> <nomÉquipe ...> {ci}: ajoute un joueur dans "
"l'équipe donnée. Le nom de l'équipe est sa couleur, ou le nom explicite "
"donné."

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:98
msgid "{cc}/bos team leave <player> {ci}: removes a player from his team."
msgstr "{cc}/bos team leave <joueur> {ci}: retire un joueur de son équipe."

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:99
msgid "{cc}/bos team list {ci}: lists the teams and their players."
msgstr "{cc}/bos team list {ci}: liste toutes les équipes et leurs membres."

#: src/main/java/eu/carrade/amaury/BallsOfSteel/commands/TeamsCommand.java:100
msgid "{cc}/bos team reset {ci}: removes all teams."
msgstr "{cc}/bos team reset {ci}: supprime toutes les équipes."

